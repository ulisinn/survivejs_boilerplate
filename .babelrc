{
    "plugins": [
        "syntax-dynamic-import"
    ],
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "IE 8"
                    ]
                }
            }
        ],
        "react",
        "es2015"
    ],
    "env": {
        "karma": {
            "plugins": [
                [
                    "istanbul",
                    {
                        "exclude": [
                            "tests/*.test.js"
                        ]
                    }
                ]
            ]
        }
    }
}
